<div class="mt-4">
  <h3>Dang khoa hoc</h3>
  <form method="POST" action="/courses/store">
    <div class="mb-3">
      <label for="name">Ten khoa hoc</label>
      <input type="text" name="name" id="name" />
    </div>

    <div class="mb-3">
      <label for="description">Mo ta</label>
      <input type="text" name="description" id="description" />
    </div>

    <div class="mb-3">
      <label for="videoId">VideoId</label>
      <input type="text" name="videoId" id="videoId" />
    </div>

    <div class="mb-3">
      <label for="level">Trinh do</label>
      <input type="text" name="level" id="level" />
    </div>
    <button type="submit" class="btn btn-primary">Them khoa hoc</button>
  </form>
</div>